{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "linebreak-style": 0,
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "no-mixed-operators": 0,
    "global-require": 0,
    "no-case-declarations": 0,
    "newline-before-return": 0,
    "no-class-assign": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-nested-ternary": 0,
    "import/extensions": 0,
    "import/no-dynamic-require": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-webpack-loader-syntax": 0,
    "import/prefer-default-export": 0,
    "react/no-danger": 0,
    "react/prefer-stateless-function": 0,
    "react/no-array-index-key": 0,
    "react/sort-comp": 0,
    "react/prop-types": 0,
    "react/jsx-indent": [
      2,
      4
    ],
    "react/jsx-filename-extension": 0,
    "react/jsx-indent-props": [
      2,
      4
    ]
  }
}